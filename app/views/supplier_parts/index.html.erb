<p id="notice"><%= notice %></p>

<h1>Supplier Parts</h1>

<table>
  <thead>
    <tr>
      <th>Part number</th>
      <th>Name</th>
      <th>Description</th>
      <th>Category code</th>
      <th>Supplier cost</th>
      <th>History</th>
      <th>Supplier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supplier_parts.each do |supplier_part| %>
      <tr>
        <td><%= supplier_part.part_number %></td>
        <td><%= supplier_part.name %></td>
        <td><%= supplier_part.description %></td>
        <td><%= supplier_part.category_code %></td>
        <td><%= supplier_part.supplier_cost %></td>
        <td><%= supplier_part.history %></td>
        <td><%= supplier_part.supplier %></td>
        <td><%= link_to 'Show', supplier_part %></td>
        <td><%= link_to 'Edit', edit_supplier_part_path(supplier_part) %></td>
        <td><%= link_to 'Destroy', supplier_part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supplier Part', new_supplier_part_path %>
